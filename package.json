{
  "name": "evm-labels",
  "version": "0.0.3",
  "description": "A public dataset of Etherscan labels",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "vitest": "vitest",
    "test": "concurrently -g -n \"eslint,vitest,build\" -c \"#341BAB,#A6CC66,#3077C6\" \"bun run lint\" \"bun run vitest -- --run\" \"bun run build\"",
    "lint": "eslint . --fix --cache",
    "combine-data": "npx tsx scripts/combine.ts && git add \"data/*/*/combined.json\" && bun run markdown-magic",
    "build": "bun run tsc:esm",
    "pull": "tsx scripts/fetch-all.ts",
    "prepare": "husky",
    "tsc:esm": "tsc -p tsconfig.json",
    "postversion": "git push --follow-tags",
    "prepublishOnly": "bun run build",
    "markdown-magic": "npx tsx docs/markdown-magic.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dawsbot/evm-labels.git"
  },
  "keywords": [],
  "author": "",
  "bugs": {
    "url": "https://github.com/dawsbot/evm-labels/issues"
  },
  "homepage": "https://github.com/dawsbot/evm-labels#readme",
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.5",
    "@types/markdown-magic": "^1.0.4",
    "@types/node": "^20.11.30",
    "cheerio": "^1.0.0-rc.12",
    "cli-progress": "^3.12.0",
    "concurrently": "^8.2.2",
    "csv-parse": "^5.0.4",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "markdown-magic": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.42.1",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "rollup": "^4.13.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.3.1",
    "vitest": "^1.4.0",
    "zod": "^3.22.4"
  },
  "lint-staged": {
    "./data/**/*.json": "bun run combine-data",
    "*": "prettier -u --write"
  }
}
